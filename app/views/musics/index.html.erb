<h1>Listing musics</h1>

<table>
  <tr>
    <th>Author</th>
    <th>Description</th>
    <th>Downloadable</th>
    <th>Filename</th>
    <th>Length</th>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @musics.each do |music| %>
  <tr>
    <td><%= music.author %></td>
    <td><%= music.description %></td>
    <td><%= music.downloadable %></td>
    <td><%= music.filename %></td>
    <td><%= music.length %></td>
    <td><%= music.title %></td>
    <td><%= link_to 'Show', music %></td>
    <td><%= link_to 'Edit', edit_music_path(music) %></td>
    <td><%= link_to 'Destroy', music, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Music', new_music_path %>
